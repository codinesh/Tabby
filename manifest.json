{
  "manifest_version": 3,
  "name": "Tabby - AI Tab Manager",
  "version": "1.0.0",
  "description": "AI powered browser tab manager to organize and group your tabs. Save time and reduce tab clutter.",
  "permissions": ["tabs", "storage", "tabGroups"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "author": "Tabby",
  "homepage_url": "https://github.com/yourusername/tabby",
  "minimum_chrome_version": "88",
  "offline_enabled": true,
  "short_name": "Tabby"
}
